<?php

for ($i= 0; $i<10; $i++) {
    for ($j=0; $j<$i; $j++) {
         echo "0";
         
   }
   echo "<br>";
}
?> 



<?php
$i = 0;
while ($i<10){
    $j=0;
    while( $j < $i) {
    echo "0";
    $j++;

}
echo "<br>";
$i++;
}

?>




